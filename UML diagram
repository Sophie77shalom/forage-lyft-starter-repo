+------------------------------------+
|            CarService              |
+------------------------------------+
| - serviceCriteria: Map<String, Integer>   |
| - tireCriteria: int                      |
+------------------------------------+
| + shouldServiceCar(car: Car): boolean     |
+------------------------------------+
                     ^
+--------------------------------------+
|                  Car                 |
+--------------------------------------+
| - carModel: String                    |
| - parts: List<Part>                   |
+--------------------------------------+
| + getCarModel(): String               |
| + getParts(): List<Part>              |
| + addPart(part: Part)                 |
+--------------------------------------+

                     ^
+--------------------------------------+
|                  Part                  |
+--------------------------------------+
| - name: String                        |
| - criteria: int                        |
+--------------------------------------+
| + getName(): String                   |
| + getCriteria(): int                   |
| + setCriteria(criteria: int)          |
+--------------------------------------+
